<UserControl x:Class="WpfApp1.Pages.InventoryPage"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:WpfApp1.Pages"
             xmlns:vm="clr-namespace:WpfApp1.ViewModels"
             xmlns:converters="clr-namespace:WpfApp1.Converters" 
             mc:Ignorable="d"
             d:DataContext="{d:DesignInstance Type=vm:InventoryViewModel, IsDesignTimeCreatable=True}"
             d:DesignHeight="800" d:DesignWidth="1200"
             Background="{DynamicResource MaterialDesignPaper}"
             Foreground="{DynamicResource MaterialDesignBody}">

    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </UserControl.Resources>

    <Grid>
        <TextBlock Text="Управление Инвентарем (Это InventoryPage) - ЗДЕСЬ ВАША РАЗМЕТКА" Style="{StaticResource MaterialDesignHeadline5TextBlock}" Margin="16"/>

    </Grid>
</UserControl>